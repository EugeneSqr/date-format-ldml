{
  "name": "date-format-ldml",
  "version": "2.1.0",
  "description": "A date formatting library based on Steven Levithan's Date Format 1.2.3, that attempts to stay closer to Unicode locale data markup language standard",
  "main": "src/date-format-ldml.js",
  "scripts": {
    "test": "node_modules/karma/bin/karma start tests/karma.conf.js",
    "build": "node_modules/uglify-js/bin/uglifyjs src/date-format-ldml.js -o ./dist/date-format-ldml.min.js",
    "lint": "node node_modules/eslint/bin/eslint.js src/date-format-ldml.js --fix"
  },
  "keywords": [
    "date",
    "time",
    "format",
    "unicode"
  ],
  "author": "eugenesqr",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.19.1",
    "jasmine": "^3.1.0",
    "karma": "^2.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "uglify-js": "^3.3.16"
  }
}
